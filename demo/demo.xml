<odoo>
    <!-- Demo data for stickers.shape -->
    <record id="shape_rectangle" model="stickers.shape">
        <field name="name">Rectangle</field>
    </record>
    <record id="shape_square" model="stickers.shape">
        <field name="name">Square</field>
    </record>
    <record id="shape_circle" model="stickers.shape">
        <field name="name">Circle</field>
    </record>
    <record id="shape_star" model="stickers.shape">
        <field name="name">Star</field>
    </record>
    <record id="shape_triangle" model="stickers.shape">
        <field name="name">Triangle</field>
    </record>

    <!-- Demo data for stickers.color -->
    <record id="color_red" model="stickers.color">
        <field name="color_name">Red</field>
        <field name="category">1</field>
    </record>
    <record id="color_orange" model="stickers.color">
        <field name="color_name">Orange</field>
        <field name="category">2</field>
    </record>
    <record id="color_yellow" model="stickers.color">
        <field name="color_name">Yellow</field>
        <field name="category">3</field>
    </record>
    <record id="color_green" model="stickers.color">
        <field name="color_name">Green</field>
        <field name="category">4</field>
    </record>
    <record id="color_blue" model="stickers.color">
        <field name="color_name">Blue</field>
        <field name="category">5</field>
    </record>
    <record id="color_purple" model="stickers.color">
        <field name="color_name">Purple</field>
        <field name="category">6</field>
    </record>
    <record id="color_brown" model="stickers.color">
        <field name="color_name">Brown</field>
        <field name="category">7</field>
    </record>
    <record id="color_gray" model="stickers.color">
        <field name="color_name">Gray</field>
        <field name="category">8</field>
    </record>
    <record id="color_black" model="stickers.color">
        <field name="color_name">Black</field>
        <field name="category">9</field>
    </record>
    <record id="color_white" model="stickers.color">
        <field name="color_name">White</field>
        <field name="category">10</field>
    </record>

    <!-- Demo data for stickers.scale -->
    <record id="scale_centimetros" model="stickers.scale">
        <field name="name">centimetros</field>
    </record>
    <record id="scale_metros" model="stickers.scale">
        <field name="name">metros</field>
    </record>

    <!-- Demo data for stickers.printing -->
    <record id="printing_glossy" model="stickers.printing">
        <field name="name">Glossy</field>
    </record>
    <record id="printing_matte" model="stickers.printing">
        <field name="name">Matte</field>
    </record>
    <record id="printing_transparent" model="stickers.printing">
        <field name="name">Transparent</field>
    </record>
    <record id="printing_embossed" model="stickers.printing">
        <field name="name">Embossed</field>
    </record>
    <record id="printing_holographic" model="stickers.printing">
        <field name="name">Holographic</field>
    </record>

    <!-- Demo data for stickers.material -->
    <record id="material_vinyl" model="stickers.material">
        <field name="name">Vinyl</field>
    </record>
    <record id="material_paper" model="stickers.material">
        <field name="name">Paper</field>
    </record>

    <!-- Demo data for stickers.customized -->
    <record id="customized_sticker_1" model="stickers.customized">
        <field name="id_finish" ref="printing_glossy"/>
        <field name="id_scale" ref="scale_small"/>
        <field name="id_shape" ref="shape_rectangle"/>
        <field name="id_material" eval="[(6, 0, [ref('material_vinyl'), ref('material_paper')])]"/>
        <field name="id_color" eval="[(6, 0, [ref('color_red'), ref('color_blue')])]"/>
        <field name="width">200</field>
        <field name="height">100</field>
        <field name="message_personalized">Custom sticker message</field>

    </record>
</odoo>
